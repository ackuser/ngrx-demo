{
  "name": "ngrx-demo",
  "version": "2.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "prod-build": "npm run build -- --prod --build-optimizer --base-href=./",
    "prod-build-named-chunks": "npm run prod-build -- --named-chunks",
    "prod-build-source-map": "npm run prod-build-named-chunks -- --source-map",
    "dev-serve": "ng serve -o",
    "prod-serve": "ng serve --prod -o",
    "test": "ng test",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "lint:styles": "stylelint ./src/**/*.scss",
    "e2e": "ng e2e",
    "scss-lint": "scss-lint",
    "outdated": "npm outdated --depth 0",
    "doc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -n \"ngrx-demo documentation\"  --theme \"Vagrant\" -s -o ",
    "stats": "ng build --prod --build-optimizer --base-href=./ --stats-json && webpack-bundle-analyzer dist/hd-customer-web/stats.json",
    "conventional-changelog": "conventional-changelog",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/http": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@ngrx/effects": "~7.1.0",
    "@ngrx/entity": "~7.1.0",
    "@ngrx/router-store": "~7.1.0",
    "@ngrx/store": "~7.1.0",
    "@ngrx/store-devtools": "~7.1.0",
    "angular-in-memory-web-api": "~0.8.0",
    "core-js": "~2.6.0",
    "rxjs": "~6.3.3",
    "tslib": "~1.9.0",
    "zone.js": "~0.8.27"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.1",
    "@angular/cli": "~7.2.1",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@compodoc/compodoc": "~1.1.7",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "conventional-changelog-cli": "~2.0.11",
    "husky": "~1.3.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "~0.2.2",
    "ngrx-store-freeze": "~0.2.4",
    "protractor": "~5.4.2",
    "rxjs-tslint-rules": "~4.15.0",
    "rxjs-watcher": "~1.0.6",
    "source-map-explorer": "~1.6.0",
    "stylelint": "~9.10.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.12.0",
    "tslint-eslint-rules": "~5.4.0",
    "tslint-language-service": "~0.9.9",
    "typescript": "~3.2.2",
    "webpack-bundle-analyzer": "~3.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "ng lint",
      "pre-push": "ng lint"
    }
  }
}
